@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en"
      class="light-style layout-menu-fixed"
      dir="ltr"
      data-theme="theme-default"
      data-assets-path="../assets/"
      data-template="vertical-menu-template-free">
@await Html.PartialAsync("/Views/Admin/PartialHeader.cshtml")
<body>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme">
                @await Html.PartialAsync("/Views/Admin/PartialAppBrandDemo.cshtml")
                <div class="menu-inner-shadow"></div>
                @await Html.PartialAsync("/Views/Admin/PartialSideBar.cshtml")
            </aside>
            <div class="layout-page">
                @await Html.PartialAsync("/Views/Admin/PartialNavBar.cshtml")
                <div class="content-wrapper">
                    <div class="container-xxl flex-grow-1 container-p-y">
                        @RenderBody()
                    </div>
                    @await Html.PartialAsync("/Views/Admin/PartialFooter.cshtml")
                    <div class="content-backdrop fade"></div>
                </div>
            </div>
        </div>

        <div class="layout-overlay layout-menu-toggle"></div>
    </div>
    @await Html.PartialAsync("/Views/Admin/PartialScript.cshtml")
</body>
</html>

